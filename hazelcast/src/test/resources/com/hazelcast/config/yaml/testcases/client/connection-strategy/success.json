{
  "instance": {
    "hazelcast-client": {
      "connection-strategy": {
        "async-start": true,
        "reconnect-mode": "ASYNC",
        "connection-retry": {
          "initial-backoff-millis": 2000,
          "max-backoff-millis": 60000,
          "multiplier": 3,
          "cluster-connect-timeout-millis": 5000,
          "jitter": 0.5
        }
      }
    }
  },
  "error": null
}
