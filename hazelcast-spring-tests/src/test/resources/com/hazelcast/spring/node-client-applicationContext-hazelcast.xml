                <hz:predicate type="sql">__key > 12</hz:predicate>
                <hz:batch-size>12</hz:batch-size>
                <hz:buffer-size>33</hz:buffer-size>
                <hz:coalesce>true</hz:coalesce>
                <hz:populate>false</hz:populate>
                <hz:delay-seconds>12</hz:delay-seconds>
